#
# EXStreamTV Docker Environment Variables
#
# Copy this file to .env and fill in your values
#

# Timezone
TZ=America/New_York

# AI Provider API Keys (choose one or more)
# Groq - Get free key at https://console.groq.com
GROQ_API_KEY=

# SambaNova - Get free key at https://cloud.sambanova.ai
SAMBANOVA_API_KEY=

# OpenRouter - Get key at https://openrouter.ai
OPENROUTER_API_KEY=

# Plex Integration (optional)
PLEX_URL=http://localhost:32400
PLEX_TOKEN=

# Jellyfin Integration (optional)
JELLYFIN_URL=
JELLYFIN_API_KEY=

# Emby Integration (optional)
EMBY_URL=
EMBY_API_KEY=
